class Solution {
    public void rotate(int[] nums, int k) {
        k %=nums.length;
        int n=nums.length;
        reverseNumber(nums,0,n-1);
        reverseNumber(nums,0,k-1);
        reverseNumber(nums,k,n-1);
    }
    public void reverseNumber(int[] nums, int start, int end){
        while(start<end){
            int temp=nums[start];
            nums[start]=nums[end];
            nums[end]=temp;
            start++;
            end--;
        }
    }
}
